scopeName: 'source.transact-sql'
name: 'SQL (Transact-SQL)'
fileTypes: [
  'sql'
]
patterns: [
  {
    match: '@[a-z_]\\w+'
    name: 'variable'
  }
  {
    match: '--.*'
    name: 'comment'
  }
  {
    begin: '\''
    end: '\''
    name: 'string'
  }
  {
    match: '\\b[0-9]+(\\.[0-9]+)?\\b'
    name: 'numeric'
  }
  {
    match: '\\b(null|not null)\\b'
    name: 'null'
  }
]
